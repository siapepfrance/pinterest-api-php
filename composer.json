{
    "name": "siapepfrance/pinterest-php-client",
    "description": "PHP client for the Pinterest API",
    "authors": [
        {
            "name": "SIAPEP France",
            "email": "contact@siapep.fr",
            "homepage": "https://www.siapep.fr",
            "role": "Creator/Developer"
        }
    ],
    "license": "Apache-2.0",
    "type": "library",
